<Response>
    {% if text_or_audio %}{% if text_or_audio.is_url %}<Play>{{ text_or_audio.value }}</Play> {% else %}<Say voice="female">{{ text_or_audio.value }}</Say>{% endif %} {% endif %}
    {% if reason %}<Reject reason="{{ reason }}"></Reject>{% endif %}
    {% if not text_or_audio and not reason %}<Hangup/>{% endif %}
</Response>